    <script>
        document.addEventListener('DOMContentLoaded', function() {
            const tabButtons = document.querySelectorAll('.tab-btn');
            const iframeContainers = document.querySelectorAll('.iframe-container');
            const noContentMessage = document.querySelector('.no-content');
            
            // åˆå§‹åŒ–ï¼šæ˜¾ç¤ºç¬¬ä¸€ä¸ªiframeï¼Œéšè—æ— å†…å®¹æ¶ˆæ¯
            noContentMessage.style.display = 'none';
            
            // ä¸ºæ¯ä¸ªæŒ‰é’®æ·»åŠ ç‚¹å‡»äº‹ä»¶
            tabButtons.forEach(button => {
                button.addEventListener('click', function() {
                    const url = this.getAttribute('data-url');
                    
                    // ç§»é™¤æ‰€æœ‰æŒ‰é’®çš„activeç±»
                    tabButtons.forEach(btn => btn.classList.remove('active'));
                    
                    // ä¸ºå½“å‰ç‚¹å‡»çš„æŒ‰é’®æ·»åŠ activeç±»
                    this.classList.add('active');
                    
                    // éšè—æ‰€æœ‰iframeå®¹å™¨
                    iframeContainers.forEach(container => {
                        container.classList.remove('active');
                    });
                    
                    // éšè—æ— å†…å®¹æ¶ˆæ¯
                    noContentMessage.style.display = 'none';
                    
                    // æ‰¾åˆ°å¯¹åº”çš„iframeå®¹å™¨
                    const index = Array.from(tabButtons).indexOf(this);
                    const targetContainer = iframeContainers[index];
                    
                    // æ˜¾ç¤ºç›®æ ‡å®¹å™¨
                    targetContainer.classList.add('active');
                    
                    // å¦‚æœiframeçš„srcæ˜¯about:blankï¼Œåˆ™åŠ è½½å¯¹åº”çš„URL
                    const iframe = targetContainer.querySelector('iframe');
                    if (iframe.src === 'about:blank' || iframe.src === '') {
                        iframe.src = url;
                    }
                });
            });
            
            // å¤„ç†iframeåŠ è½½äº‹ä»¶
            document.querySelectorAll('iframe').forEach(iframe => {
                iframe.addEventListener('load', function() {
                    // å¯ä»¥åœ¨è¿™é‡Œæ·»åŠ åŠ è½½å®Œæˆçš„å¤„ç†ï¼Œå¦‚ç§»é™¤åŠ è½½æŒ‡ç¤ºå™¨
                });
                
                iframe.addEventListener('error', function() {
                    // å¤„ç†åŠ è½½é”™è¯¯
                    console.error('æ— æ³•åŠ è½½iframeå†…å®¹:', this.src);
                });
            });
        });
    </script>
