                <a href="#"><i class="fas fa-rss"></i></a>
            </div>
        </footer>
    </div>

    <script>
        document.addEventListener("DOMContentLoaded", function () {
            // 搜索功能
            const searchInput = document.getElementById('search-input');
            const searchResults = document.getElementById('search-results');
            const cards = document.querySelectorAll('.card');
            const configLists = document.querySelectorAll('.config-list');
            
            // 存储所有配置数据
            const configsData = [];
            cards.forEach(card => {
                const platform = card.querySelector('.card-header h3').textContent.trim();
                const items = card.querySelectorAll('.config-list li');
                
                items.forEach(item => {
                    const link = item.querySelector('a');
                    const name = link.textContent;
                    const url = link.href;
                    
                    configsData.push({
                        name: name,
                        platform: platform,
                        url: url,
                        element: item
                    });
                });
            });
            
            // 搜索功能实现
            searchInput.addEventListener('input', function() {
                const searchTerm = this.value.toLowerCase().trim();
                let resultsCount = 0;
                
                if (searchTerm.length === 0) {
                    // 显示所有卡片和项目
                    cards.forEach(card => {
                        card.style.display = '';
                        card.querySelectorAll('.config-list li').forEach(item => {
                            item.style.display = '';
                        });
                    });
                    searchResults.textContent = '';
                    return;
                }
                
                // 首先隐藏所有卡片
                cards.forEach(card => {
                    card.style.display = 'none';
                });
                
                // 搜索匹配的配置
                configsData.forEach(config => {
                    const configName = config.name.toLowerCase();
                    const platform = config.platform.toLowerCase();
                    
                    if (configName.includes(searchTerm) || platform.includes(searchTerm)) {
                        // 显示包含匹配项的卡片
                        const card = config.element.closest('.card');
                        card.style.display = '';
                        
                        // 高亮匹配的文本
                        const link = config.element.querySelector('a');
                        const originalText = link.innerHTML;
                        
                        // 移除之前的高亮
                        link.innerHTML = originalText.replace(/<span class="highlight">(.*?)<\/span>/g, '$1');
                        
                        // 添加新的高亮
                        if (configName.includes(searchTerm)) {
                            const regex = new RegExp(`(${searchTerm})`, 'gi');
                            link.innerHTML = link.innerHTML.replace(regex, '<span class="highlight">$1</span>');
                        }
                        
                        // 显示匹配的项目
                        config.element.style.display = '';
                        resultsCount++;
                    } else {
                        // 隐藏不匹配的项目
                        config.element.style.display = 'none';
                    }
                });
                
                // 更新搜索结果计数
                if (resultsCount > 0) {
                    searchResults.textContent = `找到 ${resultsCount} 个匹配的配置`;
                } else {
                    searchResults.textContent = `没有找到匹配的配置`;
                }
                
                // 隐藏没有可见项目的卡片
                cards.forEach(card => {
                    const visibleItems = card.querySelectorAll('.config-list li[style=""]').length + 
                                        card.querySelectorAll('.config-list li:not([style])').length;
                    if (visibleItems === 0) {
                        card.style.display = 'none';
                    }
                });
            });
            
            // 配置导入提示
            const configLinks = document.querySelectorAll('.config-list a');
            configLinks.forEach(link => {
                link.addEventListener('click', function(e) {
                    e.preventDefault();
                    const configName = this.textContent;
                    const configUrl = this.href;
                    
                    // 使用SweetAlert显示导入提示
                    const importAlert = `
                    <div style="text-align:center;padding:1rem">
                        <h3 style="margin-bottom:1rem;color:#2c3e50">${configName}</h3>
                        <p style="margin-bottom:1.5rem">点击下方按钮复制配置链接，然后在您的代理工具中导入</p>
                        <div style="background:#f8f9fa;padding:1rem;border-radius:8px;margin-bottom:1.5rem;word-break:break-all">
                            ${configUrl}
                        </div>
                        <button id="copy-btn" style="display:inline-block;background:#3498db;color:white;padding:0.8rem 1.5rem;border-radius:8px;text-decoration:none;font-weight:500;border:none;cursor:pointer;margin-right:1rem">复制链接</button>
                        <a href="${configUrl}" target="_blank" style="display:inline-block;background:#2c3e50;color:white;padding:0.8rem 1.5rem;border-radius:8px;text-decoration:none;font-weight:500">直接打开</a>
                    </div>
                    `;
                    
                    const swalInstance = Swal.fire({
                        html: importAlert,
                        showCloseButton: true,
                        showConfirmButton: false,
                        width: 600,
                        background: '#f8f9fa',
                        didOpen: () => {
                            document.getElementById('copy-btn').addEventListener('click', function() {
                                navigator.clipboard.writeText(configUrl).then(() => {
                                    Swal.showValidationMessage('链接已复制到剪贴板！');
                                });
                            });
                        }
                    });
                });
            });
        });
    </script>
    <script src="https://cdn.jsdelivr.net/npm/sweetalert2@11"></script>
</body>
</html>