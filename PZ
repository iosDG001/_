    <script>
        document.addEventListener("DOMContentLoaded", function () {
            // 修复配置名称显示问题
            const originalConfigs = {};
            
            // 折叠功能
            const sectionHeaders = document.querySelectorAll('.section-header');
            
            sectionHeaders.forEach(header => {
                header.addEventListener('click', function() {
                    const section = this.parentElement;
                    section.classList.toggle('collapsed');
                });
            });
            
            // 展开所有按钮
            document.getElementById('expand-all').addEventListener('click', function() {
                document.querySelectorAll('.section').forEach(section => {
                    section.classList.remove('collapsed');
                });
            });
            
            // 折叠所有按钮
            document.getElementById('collapse-all').addEventListener('click', function() {
                document.querySelectorAll('.section').forEach(section => {
                    section.classList.add('collapsed');
                });
            });
            
            // 搜索功能
            const searchInput = document.getElementById('search-input');
            const searchResults = document.getElementById('search-results');
            const configLists = document.querySelectorAll('.config-list');
            
            // 存储所有配置数据
            const configsData = [];
            configLists.forEach(list => {
                const items = list.querySelectorAll('li');
                
                items.forEach(item => {
                    const link = item.querySelector('a');
                    const name = link.textContent;
                    const url = link.href;
                    
                    // 保存原始文本内容
                    originalConfigs[url] = name;
                    
                    configsData.push({
                        name: name,
                        url: url,
                        element: item
                    });
                });
            });
            
            // 搜索功能实现 - 优化响应性能
            let searchTimeout;
            searchInput.addEventListener('input', function() {
                clearTimeout(searchTimeout);
                searchTimeout = setTimeout(() => {
                    const searchTerm = this.value.toLowerCase().trim();
                    let resultsCount = 0;
                    
                    if (searchTerm.length === 0) {
                        // 显示所有项目
                        configsData.forEach(config => {
                            config.element.style.display = '';
                            const link = config.element.querySelector('a');
                            link.innerHTML = originalConfigs[config.url]; // 恢复原始名称
                        });
                        searchResults.textContent = '';
                        return;
                    }
                    
                    // 搜索匹配的配置
                    configsData.forEach(config => {
                        const configName = config.name.toLowerCase();
                        
                        if (configName.includes(searchTerm)) {
                            // 显示包含匹配项的section
                            const section = config.element.closest('.section');
                            section.classList.remove('collapsed');
                            
                            // 高亮匹配的文本
                            const link = config.element.querySelector('a');
                            const originalText = originalConfigs[config.url];
                            
                            // 移除之前的高亮
                            link.innerHTML = originalText.replace(/<span class="highlight">(.*?)<\/span>/g, '$1');
                            
                            // 添加新的高亮
                            const regex = new RegExp(`(${searchTerm})`, 'gi');
                            link.innerHTML = originalText.replace(regex, '<span class="highlight">$1</span>');
                            
                            // 显示匹配的项目
                            config.element.style.display = '';
                            resultsCount++;
                        } else {
                            // 隐藏不匹配的项目
                            config.element.style.display = 'none';
                        }
                    });
                    
                    // 更新搜索结果计数
                    if (resultsCount > 0) {
                        searchResults.textContent = `找到 ${resultsCount} 个匹配的配置`;
                    } else {
                        searchResults.textContent = `没有找到匹配的配置`;
                    }
                }, 150); // 延迟150ms执行搜索，提高响应性能
            });
            
            // 配置导入提示
            const configLinks = document.querySelectorAll('.config-list a');
            configLinks.forEach(link => {
                link.addEventListener('click', function(e) {
                    e.preventDefault();
                    const configUrl = this.href;
                    // 使用原始配置名称而不是高亮后的名称
                    const configName = originalConfigs[configUrl] || this.textContent;
                    
                    // 使用SweetAlert显示导入提示
                    const importAlert = `
                    <div style="text-align:center;padding:0.8rem">
                        <h3 style="margin-bottom:0.8rem;color:#2c3e50">${configName}</h3>
                        <p style="margin-bottom:1rem">点击下方按钮复制配置链接</p>
                        <div style="background:#f8f9fa;padding:0.8rem;border-radius:6px;margin-bottom:1rem;word-break:break-all;font-size:0.8rem">
                            ${configUrl}
                        </div>
                        <button id="copy-btn" style="display:inline-block;background:#3498db;color:white;padding:0.6rem 1.2rem;border-radius:6px;text-decoration:none;font-weight:500;border:none;cursor:pointer;margin-right:0.5rem;font-size:0.8rem">复制链接</button>
                        <a href="${configUrl}" target="_blank" style="display:inline-block;background:#2c3e50;color:white;padding:0.6rem 1.2rem;border-radius:6px;text-decoration:none;font-weight:500;font-size:0.8rem">直接打开</a>
                    </div>
                    `;
                    
                    const swalInstance = Swal.fire({
                        html: importAlert,
                        showCloseButton: true,
                        showConfirmButton: false,
                        width: window.innerWidth > 768 ? 500 : '90%',
                        background: '#f8f9fa',
                        didOpen: () => {
                            document.getElementById('copy-btn').addEventListener('click', function() {
                                navigator.clipboard.writeText(configUrl).then(() => {
                                    Swal.showValidationMessage('链接已复制到剪贴板！');
                                });
                            });
                        }
                    });
                });
            });
            
            // 页面加载后显示TG频道提示
            setTimeout(() => {
                Swal.fire({
                    title: '加入官方TG频道',
                    html: `
                    <div style="text-align:center;padding:0.8rem">
